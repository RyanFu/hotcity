.district-section-wrap {
    position: absolute;
    width: 100%;
    height: 100vh;
    top: 0;
    left: 0;
    z-index: 10;
    background: #fefefe;
}
.district-section {
    width: 100%;
}
#province,
#city,
#district {
    box-sizing: border-box;
    text-align: left;
}
.district_name {
    height: 80rpx;
    line-height: 80rpx;
    font-size: 34rpx;
    padding: 0 26rpx;
    font-family: '微软雅黑';
    color: #666;
    border-bottom: 1px solid #ccc;
}
